{
    "name": "wordle",
    "version": "1.0.0",
    "description": "wordle game",
    "private": true,
    "scripts": {
        "start": "ts-node ./node_modules/webpack-cli/bin/cli.js serve --open --config webpack/webpack.dev.ts",
        "build:dev": "NODE_ENV=development ts-node ./node_modules/webpack-cli/bin/cli.js --config webpack/webpack.dev.ts",
        "build:prod": "NODE_ENV=production ts-node ./node_modules/webpack-cli/bin/cli.js --config webpack/webpack.prod.ts",
        "prettier": "prettier --write .",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
        "lint:fix": "pnpm run lint --fix",
        "stylelint": "stylelint \"src/**/*.{css,scss}\" --fix",
        "ts:check": "pnpm exec tsc --noEmit",
        "storybook": "storybook dev -p 6006",
        "build:storybook": "storybook build"
    },
    "keywords": [],
    "author": "imgrau",
    "license": "ISC",
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "pnpm run lint:fix"
        ],
        "*.{scss,css}": [
            "pnpm run prettier",
            "pnpm run stylelint"
        ],
        "*.{json,md}": [
            "pnpm run prettier"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-react": "^7.27.1",
        "@babel/preset-typescript": "^7.27.1",
        "@storybook/addon-a11y": "^9.0.13",
        "@storybook/addon-docs": "^9.0.12",
        "@storybook/addon-onboarding": "^9.0.12",
        "@storybook/addon-styling-webpack": "^2.0.0",
        "@storybook/addon-themes": "^9.0.12",
        "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
        "@storybook/react-webpack5": "^9.0.12",
        "@types/node": "^24.0.3",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "autoprefixer": "^10.4.21",
        "babel-loader": "^10.0.0",
        "clsx": "^2.1.1",
        "core-js": "^3.43.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-import-resolver-typescript": "^4.4.3",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-storybook": "^9.0.12",
        "html-loader": "^5.1.0",
        "html-webpack-plugin": "^5.6.3",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "mini-css-extract-plugin": "^2.9.2",
        "postcss": "^8.5.6",
        "postcss-loader": "^8.1.1",
        "postcss-scss": "^4.0.9",
        "prettier": "^3.5.3",
        "sass": "^1.89.2",
        "sass-loader": "^16.0.5",
        "storybook": "^9.0.12",
        "style-loader": "^4.0.0",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-order": "^5.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "webpack": "^5.99.9",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-merge": "^6.0.1"
    },
    "dependencies": {
        "globals": "^16.2.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router": "^7.6.2",
        "react-router-dom": "^7.6.2"
    }
}
